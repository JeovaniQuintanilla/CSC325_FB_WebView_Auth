<AnchorPane id="AnchorPane" prefHeight="548.0" prefWidth="496.0" stylesheets="@../../../Styles/myStyle.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="modelview.SignInController">
   <children>
      <Button fx:id="SignInBtn" layoutX="211.0" layoutY="323.0" mnemonicParsing="false" onAction="#verifyUser" text="SignIn">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label layoutX="112.0" layoutY="215.0" text="Email:">
         <font>
            <Font size="13.0" />
         </font></Label>
      <TextField fx:id="EmailField" layoutX="131.0" layoutY="235.0" prefHeight="26.0" prefWidth="221.0" />
      <Label layoutX="345.0" layoutY="502.0" onMouseClicked="#toSignUpWindow" text="Click Here to Sign Up" textFill="#6396e1" />
      <Label layoutX="248.0" layoutY="502.0" text="Need to Register?" />
      <MenuBar layoutY="1.0" prefHeight="26.0" prefWidth="496.0">
        <menus>
          <Menu mnemonicParsing="false" text="Menu">
            <items>
              <MenuItem fx:id="closeProgram" mnemonicParsing="false" onAction="#closeProgram" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="103.0" layoutY="124.0" text="The Ultimate Database">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <Label fx:id="errorLabel" layoutX="220.0" layoutY="430.0" />
   </children>
</AnchorPane>
